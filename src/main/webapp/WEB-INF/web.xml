  </servlet>
  <servlet>
  	<servlet-name>FitbitFavoriteActivities</servlet-name>
  	<servlet-class>com.introsde.fitbit.FitbitFavoriteActivities</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FitbitWeightLogs</servlet-name>
  	<servlet-class>com.introsde.fitbit.FitbitWeightLogs</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FitbitProfile</servlet-name>
  	<display-name>FitbitProfile</display-name>
  	<description></description>
  	<servlet-class>com.introsde.fitbit.FitbitProfile</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Bmi</servlet-name>
  	<url-pattern>/Bmi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FitbitSignin</servlet-name>
  	<url-pattern>/FitbitSignin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FitbitCallback</servlet-name>
  	<url-pattern>/FitbitCallback</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FitbitUserInfo</servlet-name>
  	<url-pattern>/FitbitUserInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WeatherForecast</servlet-name>
  	<url-pattern>/WeatherForecast</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Weather</servlet-name>
  	<url-pattern>/Weather</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FitbitActivities</servlet-name>
  	<url-pattern>/FitbitActivities</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FitbitBodyGoal</servlet-name>
