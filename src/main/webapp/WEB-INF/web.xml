<web-app>
  <display-name>Adapter Service</display-name>
  <servlet>
  	<servlet-name>Bmi</servlet-name>
  	<servlet-class>com.introsde.bmi.Bmi</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FitbitSignin</servlet-name>
  	<servlet-class>com.introsde.fitbit.FitbitSignin</servlet-class>
