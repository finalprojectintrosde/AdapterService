  	<servlet-class>com.introsde.fitbit.FitbitCallback</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FitbitUserInfo</servlet-name>
  	<servlet-class>com.introsde.fitbit.FitbitUserInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WeatherForecast</servlet-name>
  	<servlet-class>com.introsde.weather.WeatherForecast</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Weather</servlet-name>
  	<servlet-class>com.introsde.weather.Weather</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FitbitActivities</servlet-name>
